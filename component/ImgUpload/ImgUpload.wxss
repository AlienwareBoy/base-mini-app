.Upload image {
  width: 150rpx;
  height: 150rpx;
}

page {
  background: #f5f5f5;
  padding-top: 30rpx;
}

.Upload {
  background: #fff;
}

.addImg {
  width: 149rpx;
  height: 149rpx;
  border: 1rpx rgba(153, 153, 153, 0.7) dashed;
  position: relative;
}

.addImg text:nth-child(1) {
  position: absolute;
  left: 0rpx;
  top: 0rpx;
  bottom: 0rpx;
  right: 0rpx;
  margin: auto;
  width: 50rpx;
  height: 4rpx;
  background: #999;
}

.addImg text:nth-child(2) {
  position: absolute;
  left: 0rpx;
  top: 0rpx;
  bottom: 0rpx;
  right: 0rpx;
  margin: auto;
  width: 4rpx;
  height: 50rpx;
  background: #999;
}

.img-item {
  margin-right: 20rpx;
  border: 1rpx solid #999;
  width: 150rpx;
  height: 150rpx;
  margin-bottom: 20rpx;
}

.img-item image {
  width: 100%;
  height: 100%;
}
.row{
  display: flex;
  justify-content: center;
  align-items: center;
}
.row_start{
  display: flex;
  justify-content: flex-start;
  align-items: flex-start;
  flex-wrap: wrap;
}
.between {
  display: flex;
  justify-content: space-between;
  align-items: center;
  flex-wrap: wrap;
}